  function validarCPFCNPJU() {
    var documentou = document.getElementById('documentou');
    var confdocumentou = documentou.value;
    var numerou = confdocumentou.replace(/\D/g, '');
    
    if (numerou.length === 11) {
        if (/^(\d)\1*$/.test(numerou)) {
            return false;
        }
        
        let soma = 0;
        for (let i = 0; i < 9; i++) {
            soma += parseInt(numerou[i]) * (10 - i);
        }
        
        const digito1 = (soma % 11 < 2) ? 0 : 11 - (soma % 11);
        
        soma = 0;
        for (let i = 0; i < 10; i++) {
            soma += parseInt(numerou[i]) * (11 - i);
        }
        
        const digito2 = (soma % 11 < 2) ? 0 : 11 - (soma % 11);
        
        if (numerou[9] == digito1 && numerou[10] == digito2) {
            return true;
        }
    } else if (numerou.length === 14) {
        if (/^(\d)\1*$/.test(numerou)) {
            return false;
        }
        
        let soma = 0;
        let multiplicador = 5;
        for (let i = 0; i < 12; i++) {
            soma += parseInt(numerou[i]) * multiplicador;
            multiplicador = (multiplicador === 2) ? 9 : multiplicador - 1;
        }
        
        const digito1 = soma % 11 < 2 ? 0 : 11 - (soma % 11);
        
        soma = 0;
        multiplicador = 6;
        for (let i = 0; i < 13; i++) {
            soma += parseInt(numerou[i]) * multiplicador;
            multiplicador = (multiplicador === 2) ? 9 : multiplicador - 1;
        }
        
        const digito2 = soma % 11 < 2 ? 0 : 11 - (soma % 11);
        
        if (numerou[12] == digito1 && numerou[13] == digito2) {
            return true;
        }
    }
    
    return false;
}

